{
  "name": "proshop",
  "version": "1.0.0",
  "description": "MERN Stack Ecommerce app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client \"",
    "data:import": "node backend/dataSeeder.js",
    "data:destroy": "node backend/dataSeeder.js --delete"
  },
  "keywords": [
    "MERN",
    "full-stack",
    "Ecommerce"
  ],
  "author": "Alsharif-Suhaim",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.4"
  },
  "devDependencies": {
    "concurrently": "^9.1.0",
    "dotenv": "^16.4.7",
    "nodemon": "^3.1.7"
  }
}
